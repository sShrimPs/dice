<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SubActivity1">

    <fragment android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/map"
        android:name="com.naver.maps.map.MapFragment" />

    <Button
        android:id="@+id/mainbtn_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="메뉴로 돌아가기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/addbtn_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:text="주소1 복사"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/addbtn_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:text="주소2 복사"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/addbtn_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:text="주소3 복사"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/parkspot1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:text="가배정"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/mainbtn_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/parkspot2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:text="가배정"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/mainbtn_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/parkspot3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:text="가배정"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/mainbtn_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


    <Button
        android:id="@+id/parkspotclose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:text="가배정 닫기"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/parkspot1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


    <ImageView
        android:id="@+id/park_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/A1spot"
        app:srcCompat="@drawable/parkimage"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/A1spot"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/green_circle_button"
        android:text="A1"
        android:visibility="gone"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="350dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/A2spot"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/green_circle_button"
        android:text="A2"
        android:visibility="gone"
        android:textSize="15sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="447dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/A3spot"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/green_circle_button"
        android:text="A3"
        android:textSize="15sp"
        android:visibility="gone"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="554dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/A4spot"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/green_circle_button"
        android:text="A4"
        android:textSize="15sp"
        android:visibility="gone"
        tools:layout_editor_absoluteX="234dp"
        tools:layout_editor_absoluteY="554dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/A5spot"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/green_circle_button"
        android:text="A5"
        android:textSize="15sp"
        android:visibility="gone"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="447dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/A6spot"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/green_circle_button"
        android:text="A6"
        android:textSize="15sp"
        android:visibility="gone"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="350dp" />

</androidx.constraintlayout.widget.ConstraintLayout>